<Application
    x:Class="MonAssoce.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MonAssoce"
    xmlns:localData="using:MonAssoce.Data">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Content/Styles/CustomStyles.xaml"/>
                <ResourceDictionary Source="Common/StandardStyles.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            <x:String x:Key="AppName">Mon Asso</x:String>
            <x:String x:Key="URLData">MonAssoce;component/Data/Settings.xml</x:String>

        </ResourceDictionary>
    </Application.Resources>
</Application>
